# Environment variables untuk development
# Copy file ini ke .env.local dan isi dengan nilai yang sesuai

# Database PostgreSQL (nanti akan diisi dengan Supabase URL)
DATABASE_URL=postgresql://user:password@host:port/database

# Telegram Bot Token (dari @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Gemini AI API Key (dari Google AI Studio)
GEMINI_API_KEY=your_gemini_api_key_here

# Next.js Secret (generate random string)
NEXTAUTH_SECRET=your_random_secret_here

# Supabase (akan diisi nanti)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here